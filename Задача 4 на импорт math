def my_factorial(n):
    if not isinstance(n, int):
        raise TypeError("Факториал можно вычислить только для целых чисел")
    if n < 0:
        raise ValueError("Факториал не определен для отрицательных чисел")
    result = 1
    for i in range(1, n + 1):
        result *= i
    return result
